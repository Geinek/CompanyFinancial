<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.geinek.value">

    <class name="ManagerValue" table="ManagerTable">
        <id name="m_id" column="M_ID">
            
        </id>
        <property name="mname" column="MNAME" />
        <property name="sex" column="SEX" />
        <property name="age" column="AGE" />
        
        <property name="edu" column="EDU" />
		<property name="title" column="TITLE" />
        
       	<many-to-one name="dptv" class="DepartmentValue" column="DEP_ID" lazy="false"></many-to-one>
       	
        
    </class>

</hibernate-mapping>
